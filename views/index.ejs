<script>
  function getMexicoResetTimeText() {
    const now = new Date();
    const mexicoTime = new Date(now.toLocaleString("en-US", { timeZone: "America/Mexico_City" }));
    const resetTime = new Date(mexicoTime);
    resetTime.setHours(24, 0, 0, 0); // 12:00 AM

    const diffMs = resetTime - mexicoTime;
    const h = Math.floor(diffMs / 3600000);
    const m = Math.floor((diffMs % 3600000) / 60000);

    return `Próximo reinicio en: ${h}h ${m}min`;
  }  document.getElementById('nextResetText').textContent = getMexicoResetTimeText();
</script>

<script>
window.setTimeout(getVisit, 1000);
function getVisit() {
  fetch('/req')
    .then(res => res.json())
    .then(data => {
      document.getElementById('visits').innerText = data.total;
    });
}
